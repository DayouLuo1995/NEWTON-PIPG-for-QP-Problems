# CompilerName=msvc170
# Mode=optim
ninja_required_version = 1.3

# Basic folders
root = .
builddir = $root\build\win64
matlabdir = C:\Program Files\MATLAB\R2024a
startdir = C:\Users\HP\Documents\MATLAB\general newtonpipg\pcg_newton

# Toolchain information
cc = cl
cxx = cl
linker = link
linkerxx = link

cflags =  /c /Zc:__cplusplus /Zp8 /GR /W3 /EHs /nologo /MD /D_CRT_SECURE_NO_DEPRECATE /D_SCL_SECURE_NO_DEPRECATE /D_SECURE_SCL=0   /DMATLAB_MEX_FILE  /DMATLAB_MEX_FILE /O2 /Oy- /DNDEBUG /fp:strict /utf-8 /I "$root" /I "$startdir" /I "$root\interface" /I "$matlabdir\extern\include" /I "." -DHAVE_LAPACK_CONFIG_H -DLAPACK_COMPLEX_STRUCTURE -DMW_HAVE_LAPACK_DECLS -DMODEL=dev_newtonpipg_pcg_g_mex  

cxxflags =   /c /Zc:__cplusplus /Zp8 /GR /W3 /EHs /nologo /MD /D_CRT_SECURE_NO_DEPRECATE /D_SCL_SECURE_NO_DEPRECATE /D_SECURE_SCL=0   /DMATLAB_MEX_FILE  /DMATLAB_MEX_FILE /O2 /Oy- /DNDEBUG /fp:strict /utf-8 /I "$root" /I "$startdir" /I "$root\interface" /I "$matlabdir\extern\include" /I "." -DHAVE_LAPACK_CONFIG_H -DLAPACK_COMPLEX_STRUCTURE -DMW_HAVE_LAPACK_DECLS -DMODEL=dev_newtonpipg_pcg_g_mex  

ldflags =  /nologo    /DLL /LIBPATH:"$matlabdir\extern\lib\win64\microsoft" libmx.lib libmex.lib libmat.lib kernel32.lib user32.lib gdi32.lib winspool.lib comdlg32.lib advapi32.lib shell32.lib ole32.lib oleaut32.lib uuid.lib odbc32.lib odbccp32.lib  /out:"dev_newtonpipg_pcg_g_mex.mexw64" /LIBPATH:"$matlabdir\extern\lib\win64\microsoft" libmwblas.lib libmwlapack.lib libemlrt.lib libcovrt.lib libut.lib libmwmathutil.lib  /export:mexFunction /export:mexfilerequiredapiversion /export:emlrtMexFcnProperties /export:dev_newtonpipg_pcg_g /export:dev_newtonpipg_pcg_g_initialize /export:dev_newtonpipg_pcg_g_terminate /export:dev_newtonpipg_pcg_g_atexit /export:emlrt_marshallIn /export:b_emlrt_marshallIn /export:o_emlrt_marshallIn /export:emxFreeMatrix_cell_wrap_6 /export:emxFreeStruct_cell_wrap_6 /export:emxInitMatrix_cell_wrap_6 /export:emxInitStruct_cell_wrap_6 /export:emxFree_creal_T /export:emxInit_creal_T /export:emxEnsureCapacity_creal_T /export:emxEnsureCapacity_int16_T /export:emxFree_int16_T /export:emxInit_int16_T /export:emxEnsureCapacity_int32_T /export:emxFree_int32_T /export:emxEnsureCapacity_int8_T /export:emxEnsureCapacity_boolean_T /export:emxFree_int8_T /export:emxFree_boolean_T /export:emxInit_int32_T /export:emxInit_int8_T /export:emxInit_boolean_T /export:emxEnsureCapacity_real_T /export:emxFreeStruct_struct1_T /export:emxFree_real_T /export:emxInitStruct_struct1_T /export:emxInit_real_T

linkobjs = 

# Build Rules
rule cc
    command = $cc $cflags $in /Fo$out

rule cxx
    command = $cxx $cxxflags $in /Fo$out

rule link
    command = $linker @$out.rsp $linkobjs $ldflags
    rspfile = $out.rsp
    rspfile_content = $in

rule linkxx
    command = $linkerxx $in $linkobjs $ldflags

